{
  "name": "sigac",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build-tsc": "tsc --project tsconfig-typeorm.json",
    "start": "next start",
    "lint": "next lint",
    "typeorm": "npm run build-tsc && npx typeorm -d dist/data-source.js",
